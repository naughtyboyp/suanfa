#include<stdio.h>
#include<string.h>
void mul(int* x, int* y, int len1, int len2)
{
    int z[1005] = {0};
    int i = 0;
    int j = 0;
    for(i = 0; i<len1; i++)//从个位开始
    {
        int q = 0;
        for(j = 0; j<len2; j++)//i的每一位都乘上
        {
            z[i+j] += x[i] * y[j] + q;//乘积 + 进位 + 原来来的值 最精华的一步
            q = z[i+j] / 10;
            z[i+j]%=10;
        }
        z[i + len2] = q;//最后的结果要多一位数，结果是 0 + 进位
    }
    int len = len1 + len2;
    while(z[len-1] == 0 && len > 0)
    {
        len--;
    }
    for(i = len-1; i>= 0; i--)
    {
        printf("%d", z[i]);
    }
    printf("\n");
}
int main()
{
    char a[1005] = {0};
    while(~scanf("%s", a))
    {
        int x[1005] = {0};//字符转化为数字
        int y[1005] = {0};
        y[0] = 2;

        int len1 = strlen(a);
        int len2 = 1;
        int i = 0;
        int k = 0;
        for(i = len1-1, k = 0; i>=0; i--)//倒序存入数组
        {
            x[k++] = a[i] - '0';
        }
        mul(x, y, len1, len2);
    }
    return 0;
}
